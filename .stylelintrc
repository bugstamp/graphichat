{
  "processors": [
    [
      "stylelint-processor-styled-components",
      {
        "parserPlugins": [
          "jsx",
          "dynamicImport",
          "exportDefaultFrom",
          "classProperties"
        ]
      }
    ]
  ],
  "extends": [
    "stylelint-config-recommended",
    "stylelint-config-styled-components"
  ],
  "rules": {
    "property-no-vendor-prefix": [true, {
      "ignoreProperties": ["background-clip", "text-fill-color", "mask-image", "transform"]
    }]
  }
}
