{
  "name": "test-boilerplate",
  "version": "1.0.0",
  "author": "Kirill Grischenko",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --env development",
    "prod": "webpack --env production",
    "server": "nodemon --exec babel-node -- ./server/index.js",
    "build-server": "rm -rf ./server/build/* && babel -d ./server/build ./server/index.js",
    "build-all": "npm run prod && npm run build-server",
    "start": "node ./server/build/index.js"
  },
  "nodemonConfig": {
    "watch": [
      "server/",
      "public/",
      "/.env"
    ],
    "ignore": [
      "server/build/*"
    ],
    "ext": "js,json,gql",
    "env": {
      "NODE_ENV": "development"
    }
  },
  "dependencies": {
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.6.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "apollo-boost": "^0.1.23",
    "apollo-resolvers": "^1.4.1",
    "apollo-server-express": "^2.3.1",
    "axios": "^0.18.0",
    "bcrypt": "2.0.1",
    "core-js": "^2.6.1",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "dotenv-expand": "^4.2.0",
    "dotenv-webpack": "^1.6.0",
    "email-validator": "^2.0.4",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.0.2",
    "graphql-iso-date": "^3.6.1",
    "immutable": "^4.0.0-rc.12",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.8",
    "moment": "^2.22.2",
    "mongoose": "^5.4.0",
    "nodemailer": "^5.0.0",
    "nodemon": "^1.18.6",
    "polished": "^2.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.6.0",
    "react-hot-loader": "^4.3.12",
    "react-loadable": "^5.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.0.3",
    "styled-reset": "^1.6.0",
    "styled-theming": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-jsx-control-statements": "^4.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.8.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-styled-components": "^0.0.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.7.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.5.0",
    "stylelint-processor-styled-components": "^1.5.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "webpack-visualizer-plugin": "^0.1.11",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "main": "index.js"
}
